{
  "name": "Cookie Monster",
  "version": "0.1",
  "description": "This extension deals with cookie consent screens in the EU",
  "manifest_version": 3,
  "author": "Andreas Zoellner",
  "action": {
    "default_icon": {
      "16": "img/cookie16.png",
      "32": "img/cookie32.png",
      "48": "img/cookie48.png",
      "128": "img/cookie128.png"
    }
  },
  "icons": {
    "16": "img/cookie16.png",
    "32": "img/cookie32.png",
    "48": "img/cookie48.png",
    "128": "img/cookie128.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ]
}
